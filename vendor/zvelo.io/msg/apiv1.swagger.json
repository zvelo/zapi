{
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "schemes": [
    "https"
  ],
  "swagger": "2.0",
  "info": {
    "title": "zveloAPI",
    "contact": {
      "name": "zvelo",
      "url": "https://zvelo.com",
      "email": "support@zvelo.com"
    },
    "license": {
      "name": "zvelo Proprietary"
    },
    "version": "1"
  },
  "host": "api.zvelo.com",
  "basePath": "/",
  "paths": {
    "/v1/query": {
      "post": {
        "tags": [
          "APIv1"
        ],
        "summary": "Create new query",
        "operationId": "Query",
        "security": [
          {
            "authorization": [
              "zvelo.dataset"
            ]
          }
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/msgQueryRequests"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "$ref": "#/definitions/msgQueryReplies"
            }
          }
        }
      }
    },
    "/v1/query/{request_id}": {
      "get": {
        "tags": [
          "APIv1"
        ],
        "summary": "Results of active or unexpired query",
        "operationId": "Result",
        "security": [
          {
            "authorization": [
              "zvelo.dataset"
            ]
          }
        ],
        "parameters": [
          {
            "type": "string",
            "name": "request_id",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "$ref": "#/definitions/msgQueryResult"
            }
          }
        }
      }
    },
    "/v1/stream": {
      "get": {
        "tags": [
          "APIv1"
        ],
        "summary": "Stream returns all QueryResult messages processed by zveloAPI",
        "operationId": "Stream",
        "responses": {
          "200": {
            "description": "(streaming responses)",
            "schema": {
              "$ref": "#/definitions/msgQueryResult"
            }
          }
        }
      }
    },
    "/v1/suggest": {
      "post": {
        "tags": [
          "APIv1"
        ],
        "summary": "Suggest new datasets for a URL",
        "operationId": "Suggest",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/msgSuggestion"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "$ref": "#/definitions/protobufEmpty"
            }
          }
        }
      }
    }
  },
